simulate_paired_experiment <- function(n, prob_treatment = .5,
seed = sample.int(.Machine$integer.max, 1))
{
stopifnot(n %% 2 == 0)
set.seed(seed)
DATA <- simulate_potential_outcomes(n)
treated <- sample.int(n, n / 2)
control <- (1:n)[-treated]
DATA[treated, d := 1]
DATA[control, d := 0]
DATA[, y := d * y1 + (1 - d) * y0]
DATA[, `:=`(y1 = NULL, y0 = NULL)]
attr(DATA, "seed") <- seed
DATA
}
